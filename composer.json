{
    "name": "ycs77/botman-driver-line",
    "license": "MIT",
    "description": "Line driver for BotMan",
    "keywords": [
        "Bot",
        "BotMan",
        "Line",
        "Line SDK",
        "Line Bot"
    ],
    "homepage": "http://github.com/ycs77/botman-driver-line",
    "authors": [
        {
            "name": "Lucas Yang",
            "email": "yangchenshin77@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "botman/botman": "~2.1|~3.0"
    },
    "require-dev": {
        "botman/studio-addons": "~1.0",
        "phpunit/phpunit": "~5.0",
        "illuminate/contracts": "~5.5.0",
        "mockery/mockery": "^1.0",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "BotMan\\Drivers\\Line\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "BotMan\\Drivers\\Line\\Providers\\LineServiceProvider"
            ]
        }
    }
}
